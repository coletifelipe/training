		    /*-- EXIBIR CONTEÃDO DA LISTA 1 --*/
			 $('#mostra-opcoes-cursos-btn').on( "click", function(){

			 	if ( $('.mostra-opcoes-cursos').is(':visible') ) {

			 		$('.mostra-opcoes-cursos').hide();
			 		$(this).removeClass("abre-opcoes");

				} else {

				 	$('.mostra-opcoes > div').hide();
				 	$('div.mostra-opcoes > span').removeClass("abre-opcoes");

		         	$(this).addClass('abre-opcoes');
			 		$('.mostra-opcoes-cursos').fadeIn("slow");

		        }
			 	
			 });

			 $('#mostra-opcoes-formacoes-btn').on( "click", function(){


			 	if ( $('.mostra-opcoes-formacoes').is(':visible') ) {

			 		$('.mostra-opcoes-formacoes').hide();
			 		$(this).removeClass("abre-opcoes");

				} else {

				 	$('.mostra-opcoes > div').hide();
				 	$('div.mostra-opcoes > span').removeClass("abre-opcoes");

		         	$(this).addClass('abre-opcoes');
			 		$('.mostra-opcoes-formacoes').fadeIn("slow");

		        }
			 	
			 });

			 $('#mostra-opcoes-exames-btn').on( "click", function(){

			 	if ( $('.mostra-opcoes-exames').is(':visible') ) {

			 		$('.mostra-opcoes-exames').hide();
			 		$(this).removeClass("abre-opcoes");

				} else {

				 	$('.mostra-opcoes > div').hide();
				 	$('div.mostra-opcoes > span').removeClass("abre-opcoes");

		         	$(this).addClass('abre-opcoes');
			 		$('.mostra-opcoes-exames').fadeIn("slow");

		        }
			 	
			 });

		    $('.mostra-opcoes').on( "click", "ul > li > b", function(){

		        /*$('ul.lista1 li h3').removeClass("fechar");*/
		        $('.mostra-opcoes > ul > li > ul').slideUp( "slow");

		        if ( $(this).nextAll().is(':hidden') ) {
		            $(this)/*.addClass("fechar")*/.nextAll("ul").slideDown("slow");
		        } else {
		            $(this)/*.removeClass("fechar")*/.nextAll("ul").slideUp("slow");
		        }
		    });
