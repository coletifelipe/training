		<!-- FILTRO CALENDÁRIO PAGINA DE CURSOS E FORMAÇÕES -->
		<script type="text/javascript">
		var data;
		var local;
		var periodo;
				
				$('#data').change(function() {
				
				
                data=$('#data').val();
                periodo=$('#horario').val();
				local=$('#local').val();
				
				loadXMLDoc(data,local,periodo);
				
				});	
				$('#local').change(function() {
				
				
               data=$('#data').val();
                periodo=$('#horario').val();
				local=$('#local').val();
				
				loadXMLDoc(data,local,periodo);
				
				});	
				$('#horario').change(function() {
				
				data=$('#data').val();
                periodo=$('#horario').val();
				local=$('#local').val();
				
				loadXMLDoc(data,local,periodo);
				
				});	

        </script>
		
		<!-- AJAX CALENDÁRIO PAGINA DE CURSOS E FORMAÇÕES -->
		<script> 
			function loadXMLDoc(vdata,vuf,vperiodo)
			{
								
				var xmlhttp;
				if (window.XMLHttpRequest)
				{// code for IE7+, Firefox, Chrome, Opera, Safari
					xmlhttp=new XMLHttpRequest();
				}
				else
				{// code for IE6, IE5
					xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
				}
				xmlhttp.onreadystatechange=function()
				{
					if (xmlhttp.readyState==4 && xmlhttp.status==200)
					{
						document.getElementById("calendariobloco").innerHTML=xmlhttp.responseText;
					}
				}
				document.getElementById("calendariobloco").innerHTML = '<img src="/gif-load.gif" style="margin-left: 365px;margin-top: 30px;margin-bottom: 30px;" />';  
				xmlhttp.open("GET","https://www.trainning.com.br/ajax/data_cursos.php?id=56&data="+vdata+"&uf="+vuf+"&periodo="+vperiodo+"&id_formacao=",true);
				xmlhttp.send();
			}
		</script>
		
		<!-- FILTRO CALENDÁRIO PAGINA HOME -->
		<script type="text/javascript">
			var data;
		var local;
		var periodo;
				
				$('#dataHOME').change(function() {
				
				
                data=$('#dataHOME').val();
                periodo=$('#horarioHOME').val();
				local=$('#localHOME').val();
				
				loadXMLDocHOME(data,local,periodo);
				
				});	
				$('#localHOME').change(function() {
				
				
               data=$('#dataHOME').val();
                periodo=$('#horarioHOME').val();
				local=$('#localHOME').val();
				
				loadXMLDocHOME(data,local,periodo);
				
				});	
				$('#horarioHOME').change(function() {
				
				data=$('#dataHOME').val();
                periodo=$('#horarioHOME').val();
				local=$('#localHOME').val();
				
				loadXMLDocHOME(data,local,periodo);
				
				});	

        </script>
		
		<!-- AJAX CALENDÁRIO PAGINA HOME -->
		<script> 
			function loadXMLDocHOME(vdata,vuf,vperiodo)
			{
								
				var xmlhttp;
				if (window.XMLHttpRequest)
				{// code for IE7+, Firefox, Chrome, Opera, Safari
					xmlhttp=new XMLHttpRequest();
				}
				else
				{// code for IE6, IE5
					xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
				}
				xmlhttp.onreadystatechange=function()
				{
					if (xmlhttp.readyState==4 && xmlhttp.status==200)
					{
						document.getElementById("calendariobloco").innerHTML=xmlhttp.responseText;
					}
				}
				document.getElementById("calendariobloco").innerHTML = '<img src="/gif-load.gif" style="margin-left: 365px;margin-top: 30px;margin-bottom: 30px;" />';  
				xmlhttp.open("GET","https://www.trainning.com.br/ajax/data_home.php?id=&data="+vdata+"&uf="+vuf+"&periodo="+vperiodo,true);
				xmlhttp.send();
			}
		</script>
		
		
		
		<!-- FILTRO CALENDÁRIO PAGINA HOME -->
		<script type="text/javascript">
		var local2;
				
			
				$('#localCALENDARIO').change(function() {
				
				local2=$('#localCALENDARIO').val();
				loadXMLDocCAL(local2);
				
				});	

			function loadXMLDocCAL(local2)
			{
								
				var xmlhttp;
				if (window.XMLHttpRequest)
				{// code for IE7+, Firefox, Chrome, Opera, Safari
					xmlhttp=new XMLHttpRequest();
				}
				else
				{// code for IE6, IE5
					xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
				}
				xmlhttp.onreadystatechange=function()
				{
					if (xmlhttp.readyState==4 && xmlhttp.status==200)
					{
						document.getElementById("content-calendario").innerHTML=xmlhttp.responseText;
					}
				}
				document.getElementById("content-calendario").innerHTML = '<img src="/gif-load.gif" style="margin-left: 365px;margin-top: 30px;margin-bottom: 30px;" />';  
				xmlhttp.open("GET","https://www.trainning.com.br/ajax/data_calendario.php?uf="+local2,true);
				xmlhttp.send();
			}
		</script>
		
		
		
		<script>		
		
$(document).ready(function() {

    
    $('.formulario-contato').submit(function(event) {

        
        $.ajax({
            type        : 'POST', // define the type of HTTP verb we want to use (POST for our form)
            url         : 'https://www.trainning.com.br/form/newsletter.php', // the url where we want to POST
            data        : $(".formulario-contato").serialize(), // our data object
            dataType    : 'json', // what type of data do we expect back from the server
                        encode          : true
        })
            
            .done(function(data) {

                
                console.log(data); 
				
				
				if (data.success) {
				
				$(".formH3").html('Cadastro efetuado com sucesso!');
				$(".formH3").css( "color", "#ffeb3b" );
				$(".formulario-contato").css( "opacity", "0.2" );
				}

                // here we will handle errors and validation messages
            });

        // stop the form from submitting the normal way and refreshing the page
        event.preventDefault();
    });

});
		
		</script>
		
