$(document).ready(function() {

    
    $('.formulario-contato-receba-info').submit(function(event) {

       $(".img-loading").show();
	   $(".content-receba-info").hide();
		
        $.ajax({
            type        : 'POST', // define the type of HTTP verb we want to use (POST for our form)
            url         : 'https://www.trainning.com.br/form/receba-info.php', // the url where we want to POST
            data        : $(".formulario-contato-receba-info").serialize(), // our data object
            dataType    : 'json', // what type of data do we expect back from the server
                        encode          : true
        })
		
		
            .done(function(data) {

                 
		
                console.log(data); 
				
				
				if (data.success) {
				
				$(".img-loading").hide();
				$(".content-receba-info-final").show();
				
				

				
				
				
				
				}

                // here we will handle errors and validation messages
            });

        // stop the form from submitting the normal way and refreshing the page
        event.preventDefault();
    });

  
});
		
