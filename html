		<script type="text/javascript">
function open_modal( id ){  
    var maskHeight = $(document).height(); 
	
	if($(window).width() > 1280){ 
    var maskWidth = $(window).width();  
	}else {
	var maskWidth = 1280; 	
	
	}  
  
   // $('#mask').css({'width':maskWidth,'height':maskHeight});  
  
    $('#mask').fadeIn(1000);  
    $('#mask').fadeTo("slow",0.8);    
  
    //Get the window height and width  
    var winH = $(window).height();  
    var winW = $(window).width();  
   
    $(id).fadeIn(2000);  
};  

$(document).ready(function() {   



$('.btn-video-youtube').click(function(e) {  

	
    $('#Iframe-youtube').attr('src',$(this).attr('ref'));

	$("#video-youtube").show();
	console.log('video-youtube');


	
        

    });  


  
    $('a[name=modal]').click(function(e) {  
        e.preventDefault();  
        open_modal( $(this).attr('href') );  
    });  
	
	$("#abrir_text").click(function() {
    $("#text").fadeOut(2000);  
	});
	 $('#curso_modal option').each(function(){
           if($(this).val() == 'Proposta para Turma Fechada'){
              $("#text").fadeOut(2000);  
           }
       });
	
	
	$(".curso_select").change(function() {
     $('.curso_select option').each(function(){
           if($(".curso_select").val() == 'Proposta para Turma Fechada'){
              $("#text").fadeIn(2000);   
			  $("#mod_submit").css('left', 24).css('top',415);  
			  $("#mod_submit input").css('padding-left', 142);
			  $("#mod_submit input").css('padding-right', 142);
			  
			  $("#boxes #dialog").css('height', 520);
			  $("#sair").css('margin-top', 483);
           }
		    if($(".curso_select").val() == 'Proposta para Curso Incompany'){
              $("#text").fadeIn(2000);   
			  $("#mod_submit").css('left', 24).css('top',415);  
			  $("#mod_submit input").css('padding-left', 142);
			  $("#mod_submit input").css('padding-right', 142);
			  
			  $("#boxes #dialog").css('height', 520);
			  $("#sair").css('margin-top', 483); 
           }
		    else if($(".curso_select").val() == 'Outros Cursos e Servi?'){
              $("#text").fadeIn(2000);   
			  $("#mod_submit").css('left', 24).css('top',415);  
			  $("#mod_submit input").css('padding-left', 142);
			  $("#mod_submit input").css('padding-right', 142);
			  
			  $("#boxes #dialog").css('height', 520);
			  $("#sair").css('margin-top', 483); 
           }
		  
       });
	 	
	});

    open_modal( '#dialog' );//abrindo o div#modal ao carregar a p?na  
