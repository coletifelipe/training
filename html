		<script>		
		var carrinhophp = '';	
$(document).ready(function() {

    
    $('.add-to-cart').live('click',function(event) {

				
   // $('.load').fadeIn('fast');
   $('.modal-checkout').fadeIn('fast');
	$(this).addClass('selecionado');

	if($(this).attr('data-id')){

	//if($(this).attr('data-id').includes('f')){

	//	$('.c-curso2').fadeOut('fast');
	//	console.log('tem f');
	//}
	//else{
	//	console.log('n tem f');

	//}
}
	

		carrinhophp = carrinhophp.replace($(this).attr('data-id-retira'), '');
        
        $.ajax({
            type        : 'POST', // define the type of HTTP verb we want to use (POST for our form)
            url         : 'https://www.trainning.com.br/ajax/data_modal_insc.php', // the url where we want to POST
            data        : {carrinho:carrinhophp,carrinhonovo:$(this).attr('data-id'),cupom:$('#inputcupom').val(),tipo:$('#local').val()}, // our data object
            dataType    : 'json', // what type of data do we expect back from the server
            encode      : true
        })
            
            .done(function(data) {


				console.log($('#local').val());
				
				$(".dados-checkout").html(data.html);
                carrinhophp = data.valores;
				console.log(carrinhophp);
                variavelcupom = data.cupom;
				console.log(variavelcupom);
			//	$('.load').fadeOut('fast');
			//	$('.dados-checkout').fadeIn('fast');

				$('#nomecursocheckout').val(carrinhophp);
				$('#cupom').val(variavelcupom);

                // here we will handle errors and validation messages
            });

        // stop the form from submitting the normal way and refreshing the page
        event.preventDefault();
    });





   $('.remove-to-cart').live('click',function(event) {

		variavelretira = $(this).attr('data-id-retira');

			 //	console.log(variavelretira);
			 	$('#'+variavelretira+'').removeClass('selecionado');

//$('.add-to-cart').trigger('click');
});







});
		
		</script>










